@{
    ViewBag.Title = "Messaging | Reports";
    Layout = "~/Views/Shared/Layout/_adminSchLayout.cshtml";
}


<ul class="breadcrumb">
    <li class="breadcrumb-item"><a href="#">Messaging</a></li>
    <li class="breadcrumb-item"><a href="@Url.Action("Reports","Messaging")"> Reports </a></li>
</ul>

<div class="content-i" id="app">
    <div class="content-box">
        <div class="element-wrapper">
            <h6 class="element-header"> Reports </h6>
            <div class="element-box">
                <div class="table-responsive" style="height:auto;width:auto;min-height:412px;">

                    <div class="row">

                        <div class="col-sm-2">  </div>
                        <div class="col-sm-3">

                            <div class="form-group">
                                <label> <strong>Start Date:</strong> </label>
                                <input v-model="startDate" ref="startDate" type="date" class="form-control" v-on:change="getData()" />

                            </div>

                        </div>
                        <div class="col-sm-3">

                            <div class="form-group">
                                <label> <strong> End Date: </strong> </label>
                                <input v-model="endDate" ref="endDate" type="date" class="form-control" v-on:change="getData()" />

                            </div>

                        </div>
                    </div>

                    <hr />
                    <div style="height:auto;width:auto;min-height:500px;" id="reportView">  </div>
                </div>

            </div>
        </div>


    </div>
</div>

<script src="~/js/school-manager/messaging/reports.js" asp-append-version="true"></script>