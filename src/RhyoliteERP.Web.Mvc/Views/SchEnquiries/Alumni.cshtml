@{
    ViewBag.Title = "Enquiries | Alumni";
    Layout = "~/Views/Shared/Layout/_adminSchLayout.cshtml";
}


<ul class="breadcrumb">
    <li class="breadcrumb-item"><a href="#">Enquiries</a></li>
    <li class="breadcrumb-item"><a href="@Url.Action("Alumni","SchEnquiries")"> Alumni </a></li>
</ul>

<div class="content-i" id="app">
    <div class="content-box">
        <div class="element-wrapper">
            <h6 class="element-header"> Alumni </h6>
            <div class="element-box">
                <div class="table-responsive" style="height:auto;width:auto;min-height:500px;">

                    <div class="row">

                        <div class="col-sm-2"></div>

                        <div class="col-sm-3">

                            <div class="form-group">
                                <label> <strong> Academic Year: </strong>  </label>
                                <select class="form-control mb-3" v-model="selectedAcademicYearId" v-on:change="getData()">
                                    <option value="">Select Academic Year</option>
                                    <option :value="academicYear.id" v-for="academicYear in academicYearList"> {{academicYear.name}} </option>
                                </select>

                            </div>

                        </div>

                    </div>

                    <hr />
                    <div id="dataTable"></div>

                </div>

            </div>
        </div>


    </div>
</div>

<script src="~/js/school-manager/enquiries/alumni.js" asp-append-version="true"></script>
