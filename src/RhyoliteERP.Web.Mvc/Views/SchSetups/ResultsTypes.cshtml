@{
    ViewBag.Title = "Results Types";
    Layout = "~/Views/Shared/Layout/_adminSchLayout.cshtml";
}


<ul class="breadcrumb">
    <li class="breadcrumb-item"><a href="#">Setups</a></li>
    <li class="breadcrumb-item"><a href="@Url.Action("ResultsTypes","SchSetups")"> Results Types </a></li>
</ul>

<div class="content-i" id="app">
    <div class="content-box">
        <div class="element-wrapper">
            <h6 class="element-header"> Results Types </h6>
            <div class="element-box">
                <div class="table-responsive" style="height:auto;width:auto;min-height:412px;">

                     <div class="row">

                            <div class="col-sm-2">

                            </div>

                            <div class="col-sm-3">

                                <div class="form-group">
                                <label> <strong> Level:</strong> </label>
                                    <select class="form-control mb-3" v-model="selectedLevelId" v-on:change="getSetupData()">
                                        <option value="">Select Level</option>
                                        <option :value="level.id" v-for="level in levelList"> {{level.name}} </option>
                                    </select>
                                </div>

                            </div>

                        </div>

                        <hr />

                    <div id="dataTable"></div>

                </div>

            </div>
        </div>


    </div>
</div>

<script src="~/js/school-manager/setups/results-type.js" asp-append-version="true"></script>
